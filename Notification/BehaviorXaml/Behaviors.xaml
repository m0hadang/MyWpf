<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:beh="clr-namespace:Notification.BehaviorSrc">
    <beh:FluidMoveBehavior x:Key="BehaviorXY400" x:Shared="False" AppliesTo="Children" Duration="0:0:.4">
        <beh:FluidMoveBehavior.EaseX>
            <PowerEase/>
        </beh:FluidMoveBehavior.EaseX>
        <beh:FluidMoveBehavior.EaseY>
            <PowerEase/>
        </beh:FluidMoveBehavior.EaseY>
    </beh:FluidMoveBehavior>    
</ResourceDictionary>